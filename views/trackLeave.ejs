<% include ./partial/header %> <% include ./partial/studfunction %> 
<div
  class="col-sm-6"
  style="    flex: 0 0 50%;
max-width: 60%;
margin-left: 431px;
margin-top: -181px;border: 1px solid pink ;  text-align: -webkit-center;"
>
<div class="container" style="border: 2px solid red">
    <table style="width: 100%">
      <tr>
        <th>
          Subject
        </th>
        <th>
          From
        </th>
        <th>To</th>
        <th>Days</th>
        <th>HOD Status</th>
        <th>Warden Status</th>
        
      </tr>
      <%student.leaves.forEach(function(leave){%>

      <tr>
        <td><%=leave.subject%></td>
        <td><%=moment(leave.from).format('DD/MM/YYYY')%></td>
        <td><%=moment(leave.to).format('DD/MM/YYYY')%></td>
        <td><%=leave.days%></td>
        <td><%=leave.status%></td>
        <td><%=leave.wardenstatus%></td>
        
      </tr>

      <%})%>
    </table>
  </div>
  </div>
  







</div>










